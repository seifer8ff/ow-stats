<div id="content">
	<div class="container">
		{{> heroSelect}}
	</div>
	<div class="container hero-container">
		<div class="row" id="hero-multiple-parent">
			{{#each heroes}}
				{{#if (heroSelected this.normalizedName @root.user)}}
					{{> heroSummary hero=this}}
				{{/if}}
			{{/each}}
		</div>
	</div>
</div>